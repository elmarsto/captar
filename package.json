{
	"name": "captar",
	"version": "0.0.1",
	"description": "Captar",
	"main": "main.js",
	"scripts": {
		"check-types": "tsc --noemit",
		"dev": "cross-env BUILD=dev obsidian-plugin dev src/main.tsx",
		"build": "cross-env BUILD=production obsidian-plugin build src/main.tsx",
		"lint": "eslint ./src",
		"lint:fix": "eslint ./src --fix",
		"sort-imports": "import-sort --write \"./src/**/*.{ts,tsx}\"",
		"prettier": "prettier --write \"./src/**/*.{ts,tsx}\"",
		"clean": "npm run sort-imports && npm run prettier && npm run lint:fix"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@codemirror/state": "^6.0.0",
		"@codemirror/view": "^6.0.0",
		"@types/box-intersect": "^1.0.0",
		"@types/insert-text-at-cursor": "^0.3.0",
		"@types/mark.js": "^8.11.5",
		"@types/node": "^14.14.37",
		"@types/raf-schd": "^4.0.1",
		"@types/textarea-caret": "^3.0.1",
		"@typescript-eslint/eslint-plugin": "^5.8.1",
		"@typescript-eslint/parser": "^5.8.1",
		"cross-env": "^7",
		"eslint": "^8.5.0",
		"eslint-plugin-react": "^7.24.0",
		"import-sort-cli": "^2.4.0",
		"import-sort-style-module-alias": "^1.1.0",
		"obsidian": "^0.15.3",
		"obsidian-dataview": "^0.4.21",
		"obsidian-plugin-cli": "^0.0.5",
		"prettier": "^2.3.2",
		"replace": "^1.2.1",
		"tslib": "^2.4.0",
		"typescript": "^4.7.4"
	},
	"dependencies": {
		"animated-scroll-to": "^2.2.0",
		"box-intersect": "^1.0.2",
		"choices.js": "9.0.1",
		"classcat": "^5.0.3",
		"deepmerge": "^4.2.2",
		"eventemitter3": "^4.0.7",
		"fast-json-patch": "^3.1.0",
		"file-selector": "^0.2.4",
		"fuse.js": "^6.5.3",
		"immutability-helper": "^3.1.1",
